<div class="score-container">
  <a href="/games/<%= score.url_slug %>">
  <div class="score-text">
    <div class="flex-container">
      <div>
        <h2><%= score.title %></h2>
      </div>
      <div>
        <h2><%= score.highscore %></h2>
      </div>
    </div>
      <div>
        <p><%= score.player %>, 
          <% const scoreDate = new Date (score.played_at); %>
          <%=scoreDate.toLocaleDateString('se-SE'); %></p>
      </div> 
  </div>

  </a>
</div>
  